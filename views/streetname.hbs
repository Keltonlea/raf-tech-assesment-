
<!DOCTYPE html>
<html>
<head>
  <title>Parcel Data</title>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
  <script src="/js/streetnamebutton.js"></script>
  <script src="/js/homebutton.js"></script>
  <script src="/js/firstnamebutton.js"></script>

</head>
<body>
  <div class="container-fluid">
    <h1>Parcel Data</h1>

    <button type="button" id="streetname-button" class="btn btn-primary btn-lg btn-block">Sort Data By Street Name</button>
    <button type="button" id="firstname-button" class="btn btn-primary btn-lg btn-block">Sort Data By First Name</button>
    <button type="button" id="home-button" class="btn btn-primary btn-lg btn-block">Home</button>
    <table class="table table-striped">


  <thead>
    <tr>
      <th>Pin</th>
      <th>Address</th>
      <th>Owner</th>
      <th>Market Value</th>
      <th>Sale Date</th>
      <th>Sale Price</th>
      <th>Link</th>
      <th>Map Link</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
      {{#each this}}
        <tr>
          <td>{{ pin }}</td>
          <td>{{ address }}</td>
          <td>{{ owner }}</td>
          <td>{{ market_value }}</td>
          <td>{{ sale_date }}</td>
          <td>{{ sale_price }}</td>
          <td>{{ link }}</td>
          <td><a href="/map?address={{this.address}}">{{this.address}}</a></td>

        </tr>
      {{/each}}
    {{/each}}
  </tbody>
  

